                                                  <fo:block text-align="left">
                                                  <fo:block-container space-after="1em" max-height="100%" max-width="100%" width="550px" id="unique_7_Connect_42_image_xdc_ddv_ktb" chfox:nd-id="image_xdc_ddv_ktb"><fo:block end-indent="0" start-indent="0"><fo:inline><fo:external-graphic src="file:/C:/Users/Filip/Desktop/-Screenshots-best-practices/Images/full_screen_2.png" fox:alt-text="" max-width="100%" max-height="100%" scaling="uniform" width="550px" content-width="scale-to-fit"/></fo:inline></fo:block></fo:block-container>
