                                                  <fo:block-container space-after="1em" max-height="100%" max-width="100%" width="550px" id="unique_7_Connect_42_image_jsf_3dv_ktb" chfox:nd-id="image_jsf_3dv_ktb"><fo:block end-indent="0" start-indent="0"><fo:inline><fo:external-graphic src="file:/C:/Users/Filip/Desktop/-Screenshots-best-practices/Images/screenshot_context_1.png" fox:alt-text="" max-width="100%" max-height="100%" scaling="uniform" width="550px" content-width="scale-to-fit"/></fo:inline></fo:block></fo:block-container>
                                                  </fo:block>
                                                  </fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-color="rgb(68 , 68 , 68)" border-bottom-style="solid" border-bottom-width="0" border-right-color="rgb(68 , 68 , 68)" border-right-style="solid" border-right-width="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>
                                                  <fo:block text-align="left">
                                                  <fo:block-container space-after="1em" max-height="100%" max-width="100%" width="550px" id="unique_7_Connect_42_image_ksf_3dv_ktb" chfox:nd-id="image_ksf_3dv_ktb"><fo:block end-indent="0" start-indent="0"><fo:inline><fo:external-graphic src="file:/C:/Users/Filip/Desktop/-Screenshots-best-practices/Images/screenshot_context_2.png" fox:alt-text="" max-width="100%" max-height="100%" scaling="uniform" width="550px" content-width="scale-to-fit"/></fo:inline></fo:block></fo:block-container>
                                                  </fo:block>
                                                  </fo:block></fo:table-cell>
